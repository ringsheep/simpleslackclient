# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'SlackClient' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for SlackClient
  pod 'AFNetworking'
  pod 'SocketRocket'
  pod 'ReactiveObjC'
  pod 'Realm'
  pod 'Realm+JSON', :git => 'https://github.com/nunobaldaia/Realm-JSON'

  target 'SlackClientTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SlackClientUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
